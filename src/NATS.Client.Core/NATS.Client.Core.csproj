<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>net6.0;net8.0</TargetFrameworks>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
    <IsTrimmable>true</IsTrimmable>

    <!-- NuGet Packaging -->
    <PackageTags>pubsub;messaging</PackageTags>
    <Description>NATS client for .NET.</Description>
    <IsPackable>true</IsPackable>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.Logging.Abstractions" Version="6.0.1"/>
    <PackageReference Include="Microsoft.Extensions.Primitives" Version="6.0.0"/>
    <PackageReference Include="Microsoft.VisualStudio.Threading.Analyzers" Version="17.1.46">
      <PrivateAssets>all</PrivateAssets>
      <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
    </PackageReference>
    <PackageReference Include="System.IO.Pipelines" Version="8.0.0"/>

    <InternalsVisibleTo Include="MicroBenchmark, "/>
    <InternalsVisibleTo Include="$(AssemblyName).Tests, "/>
    <InternalsVisibleTo Include="$(AssemblyName).MemoryTests, "/>
    <InternalsVisibleTo Include="NatsBenchmark, "/>
    <InternalsVisibleTo Include="NATS.Client.JetStream, "/>
    <InternalsVisibleTo Include="NATS.Client.JetStream.Tests, "/>
    <InternalsVisibleTo Include="NATS.Client.KeyValueStore, "/>
    <InternalsVisibleTo Include="NATS.Client.KeyValueStore.Tests, "/>
    <InternalsVisibleTo Include="NATS.Client.ObjectStore, "/>
    <InternalsVisibleTo Include="NATS.Client.ObjectStore.Tests, "/>
    <InternalsVisibleTo Include="NATS.Client.Services, "/>
    <InternalsVisibleTo Include="NATS.Client.Services.Tests, "/>
    <InternalsVisibleTo Include="NATS.Client.CheckNativeAot, "/>
    <InternalsVisibleTo Include="Nats.Client.Compat, "/>
  </ItemGroup>
</Project>
